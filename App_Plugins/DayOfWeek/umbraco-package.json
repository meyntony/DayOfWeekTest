{
  "$schema": "../../umbraco-package-schema.json",
  "name": "DayOfWeek",
  "version": "15.0.0",
  "extensions": [
    {
      "type": "propertyEditorUi",
      "alias": "UmbracoDayOfWeek",
      "name": "DayOfWeek Property Editor UI",
      "element": "/App_Plugins/DayOfWeek/day-of-week.js",
      "elementName": "day-of-week-selector",
      "meta": {
        "label": "Day Of Week",
        "icon": "icon-code",
        "group": "common",
        "propertyEditorSchemaAlias": "Umbraco.Plain.String",
        "defaultData": [
          {
            "alias": "defaultStartDayOfWeek",
            "value": 1
          }
        ],
        "settings": {
          "properties": [
            {
              "alias": "defaultStartDayOfWeek",
              "label": "Default Start Day of Week",
              "description": "This is the default start day of the Week",
              "propertyEditorUiAlias": "UmbracoDayOfWeek"
            }
          ]
        }
      }
    },
        {
            "type": "localization",
            "alias": "DayOfWeek.Localize.En",
            "name": "English",
            "meta": {
              "culture": "en",
              "localizations": {
                  "dayOfWeek": {
                      "d0": "Sunday",
                      "d1": "Monday",
                      "d2": "Tuesday",
                      "d3": "Wednesday",
                      "d4": "Thursday",
                      "d5": "Friday",
                      "d6": "Saturday"
                  }
              }
            }
        },
        {
            "type": "localization",
            "alias": "DayOfWeek.Localize.Da",
            "name": "Dansk",
            "meta": {
              "culture": "da",
              "localizations": {
                  "dayOfWeek": {
                      "d0": "søndag",
                      "d1": "mandag",
                      "d2": "tirsdag",
                      "d3": "onsdag",
                      "d4": "torsdag",
                      "d5": "fredag",
                      "d6": "lørdag"
                  }
              }
            }
        }
    ]
}